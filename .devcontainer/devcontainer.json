{
  "name": "emocare",
  "dockerComposeFile": ["./docker-compose.yml"],
  "service": "app",
  "workspaceFolder": "/workspaces/emocare",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "ms-python.debugpy",
        "ms-toolsai.jupyter",
        "ms-azuretools.vscode-docker",
        "esbenp.prettier-vscode",
        "biomejs.biome-vscode",
        "bradlc.vscode-tailwindcss",
        "rickaym.manim-sideview",
        "mhutchie.git-graph",
        "github.copilot",
        "github.copilot-chat"
      ]
    }
  },
  "forwardPorts": [3001, 8080, 5433],
  "remoteUser": "root",
  "postCreateCommand": ".devcontainer/setup.sh"

}
